<h3 class="page-header">MyJoinCircle</h3>

{{#each circles}}
<div class="row">
  <div class="col s12 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">{{circles.circlename}}</span>
        <p>{{introduction}}</p>
        <p>人数: {{members.length}} 人</p>
      </div>
      <div class="card-action">
        <a href="/detail/{{_id}}">詳細</a>
        <a href="#">トークルーム</a>
      </div>
    </div>
  </div>
</div>
{{/each}}
